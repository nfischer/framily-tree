{
  "name": "framily-tree",
  "version": "0.1.0",
  "description": "A fraternity family tree",
  "main": "index.js",
  "scripts": {
    "posttest": "npm run lint",
    "test": "nyc --reporter=text --reporter=lcov mocha",
    "lint": "eslint .",
    "start": "http-server -o",
    "deploy": "deploy-gh",
