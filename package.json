  "dependencies": {
    "didyoumean": "^1.2.2",
    "jquery": "^3.6.0",
    "tinycolor2": "^1.5.1",
    "vis": "^4.21.0-EOL"
  },
  "devDependencies": {
    "deploy-gh": "^0.3.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "http-server": "^14.1.1",
    "mocha": "^11.1.0",
    "nyc": "^17.1.0",
    "should": "^13.2.3",
    "spreadsheet-to-json": "^2.0.0"
  },
  "engines": {
    "node": ">=20"
  }
